{
    "compilerOptions": {
        "target": "es5",
        "module": "CommonJS",
        "checkJs": true,
        "outDir": "dist",
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["./**/*"],
}